.iden_body_box{
	background: #ffffff;
	padding: 0rpx 25rpx;
	margin-top: 10rpx;
}
.iden_body_box .iden_in_box{
	border-bottom: solid 1px #f2f2f2;
	height: 100rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 26rpx;
	color: #333;
}
.iden_body_box .iden_in_box .iden_imgBox_in{
	width: 40rpx;
	height: 40rpx;
}
.iden_body_box .iden_in_box .iden_imgBox_in image{
	width:100%;
	height: 100%;
}