/*二维码的盒子*/
.qr_code_mb{
    width: 100vw;
    height: 100vh;
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    z-index: 99999999;
    overflow: hidden;
}
.qr_code_mb::after{
    content:'';
    position: absolute;
    width: 100vw;
    height: 100vh;
    background: #000;
    opacity: .7;
    left:0rpx;
    top: 0rpx;
}
.qr_code_mb_in{
    position: relative;
    z-index: 999;
    transform: translate(-50%,-50%);
    left: 50%;
    top: 50%;
    width: 80%;
}
.qr_code_mb_in_img{
    width: 345rpx;
    height: 345rpx;
    min-width: 345rpx;
    position: relative;
    left: 50%;
    transform: translateX(-50%);
}
.qr_code_mb_in_img image{
    width: 100%;
    height: 100%;
    position: relative;
    z-index: 99;
}
.qr_code_mb_in_img::after{
    content: '';
    width: 100%;
    height: 100%;
    background: #fff;
    opacity: .5;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
}
.qr_code_mb_in_txt{
    width: 100%;
    padding: 10rpx 0rpx;
    font-size: 26rpx;
    border-top: 2rpx #fff solid;
    border-bottom: 2rpx #fff solid;
    margin-top: 20rpx;
    color:#fff;
}
.qr_code_mb_in_tit{
    width: 100%;
    text-align: center;
    color: #fff;
    margin-top: 20rpx;
}
.qr_code_mb_in_txt_in{
    margin-top: 10rpx;
    display: flex;
    align-items: center;
}
.qr_code_mb_in_txt_in:nth-child(1){
    margin-top: 0rpx;
}
.qr_code_mb_in_txt_in_r{
    width: 30rpx;
    height: 30rpx;
    background: #e80012;
    color: #fff;
    font-size: 20rpx;
    border-radius: 50%;
    text-align: center;
}

.flex{
  display: flex;
}
.nowrap{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  width:560rpx;
}