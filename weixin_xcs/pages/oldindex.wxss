   display: flex;
   align-items: center;
   z-index:10
   
}

.home_top_rz icon {
   vertical-align: bottom;
   font-size: 14px;
   position: relative;
   /* top: -5rpx; */
}

.home_top_rz text {
   color: #666 !important;
   padding-left: 10rpx;
}

.tv_btn {
   padding: 22rpx 0 0 0;
   width: 120rpx;
   font-size: 12px;
   text-align: center;
   position: relative;
   display: block;
   margin: 0;
   box-sizing: box;
   border: none;
   text-decoration: none;
   border-radius: 0px;
   line-height: 1.8;
   -webkit-tap-highlight-color: transparent;
   overflow: hidden;
   color: #666;
   background-color: transparent;
}

.tv_btn::after {
   border: none;
}

.tv_btn [class*=icon-] {
   font-size: 18px;
   display: block;
}

.tv_btn:last-child {
   padding-right: 10rpx;
}

.home_address {
