.home_top_rz text {
  color: #fff;
  padding-left: 20rpx;
  font-size: 24rpx;
}

.activtList {
  /* position: absolute;
  top:368rpx; */
  margin-top: 88rpx;
  width: 100%;
  min-height: 600prx;
}

.tv_btn {
  padding: 22rpx 0 0 0;
  width: 120rpx;
  font-size: 12px;
  text-align: center;
  position: relative;
  display: block;
  margin: 0;
  box-sizing: box;
  border: none;
  text-decoration: none;
  border-radius: 0px;
  line-height: 1.8;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: #fff;
  background-color: transparent;
}

.tv_btn::after {
  border: none;
}

.tv_btn [class*=icon-] {
  font-size: 18px;
  display: block;
}

.tv_btn > view {
  padding-top: 8rpx;
}

.tv_btn:last-child {
  padding-right: 10rpx;
}

.attention {
  width: 36rpx;
  height: 36rpx;
}

.home_address {
  width: 100%;
  background-color: #fff;
  /* z-index: 3; *//* padding: 15rpx 0; */
  font-size: 12px;
  color: #999;
  border-bottom: 1rpx solid #eee;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
  display: flex;
}

.home_address icon {
  font-size: 32rpx;
  padding: 0 20rpx 0 30rpx;
}

.storeClicj_in {
  display: flex;
  align-items: center;
}

.home_tel {
  /* z-index:3; */
}

.home_tel icon {
  font-size: 40rpx;
  padding: 0 26rpx;
  border-left: 1px #eee solid;
}

.store_address {
  font-size: 28rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 560rpx;
}

.store-content {
  /* position: relative; */
}

.home-top-b {
  width: 100%;
  height: 200rpx;
}

.home-top-b > image {
  width: 100%;
  height: 200rpx;
}

.store-bottom {
  display: flex;
  position: absolute;
  top: 64rpx;
  height: 136rpx;
  width: 100%;
  color: #fff;
  box-sizing: border-box;
  /* z-index:3; */
}

.centerFlex-x {
  display: flex;
  justify-content: center;
}

.centerFlex-y {
  display: flex;
  align-items: center;
}

.centerFlex {
  display: flex;
  justify-content: center;
  align-items: center;
}

.store-b-t {
  width: 180rpx;
  height: 54rpx;
  color: #fff;
  background: rgba(0, 0, 0, 0.5);
  /* border-radius:40rpx; */
  position: absolute;
  right: 0rpx;
  top: 12rpx;
  font-size: 26rpx;
  /* padding-right:30rpx; */
  border-top-left-radius: 27rpx;
  border-bottom-left-radius: 27rpx;
  /* opacity: .5; */
}

.store-b-t > image {
  width: 32rpx;
  height: 21rpx;
  margin-right: 10rpx;
}

.weui-flex__item {
  display: flex;
}

.storelogo {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
  margin-left: 30rpx;
  overflow: hidden;
  min-width: 108rpx;
}

.store-name-auth {
  display: flex;
  flex-direction: column;
  width: 400rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
}

.storename {
  font-weight: 600;
  font-size: 32rpx;
  padding: 8rpx 0 10rpx 0rpx;
}

.goods_list_content:last-child {
  margin-bottom: 15rpx;
}

/* 为空 */

.null-data {
  height: 668rpx;
  background-color: #fff;
  flex-direction: column;
}

.null-data1 {
  height: 1200rpx;
  background-color: #fff;
  flex-direction: column;
}

.null-img {
  width: 233rpx;
  height: 240rpx;
}

.no_activity {
  font-size: 30rpx;
  color: #333;
  padding: 10rpx 0;
}

.go_g {
  font-size: 26rpx;
  color: #999;
}

/*二维码的盒子*/

.qr_code_mb {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 99999999;
  overflow: hidden;
}

/* .pageFooter{
  position: absolute;
  bottom: 0;
  width: 100%;
} */

.qr_code_mb::after {
  content: '';
  position: absolute;
  width: 100vw;
  height: 100vh;
  background: #000;
  opacity: 0.7;
  left: 0rpx;
  top: 0rpx;
}

.qr_code_mb_in {
  position: relative;
  z-index: 999;
  transform: translate(-50%, -50%);
  left: 50%;
  top: 50%;
  width: 80%;
}

.qr_code_mb_in_img {
  width: 345rpx;
  height: 345rpx;
  min-width: 345rpx;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}

.qr_code_mb_in_img image {
  width: 100%;
  height: 100%;
  position: relative;
  z-index: 99;
}

.qr_code_mb_in_img::after {
  content: '';
  width: 100%;
  height: 100%;
  background: #fff;
  opacity: 0.5;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}

.qr_code_mb_in_txt {
  width: 100%;
  padding: 10rpx 0rpx;
  font-size: 26rpx;
  border-top: 2rpx #fff solid;
  border-bottom: 2rpx #fff solid;
  margin-top: 20rpx;
  color: #fff;
}

.qr_code_mb_in_tit {
  width: 100%;
  text-align: center;
  color: #fff;
  margin-top: 20rpx;
}

.qr_code_mb_in_txt_in {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}

.qr_code_mb_in_txt_in:nth-child(1) {
  margin-top: 0rpx;
}

.qr_code_mb_in_txt_in_r {
  width: 30rpx;
  height: 30rpx;
  background: #e80012;
  color: #fff;
  font-size: 20rpx;
  border-radius: 50%;
  text-align: center;
}

.valign-center {
  display: flex;
  align-items: center;
}

.isFixedStyle {
  position: fixed;
  top: 0;
  z-index: 10000;
  width: 100%;
}

.recommend {
  background-color: #fff;
  margin-bottom: 20rpx;
}

.recommend_con {
  display: flex;
  padding: 20rpx 30rpx;
  justify-content: space-between;
}

.recommend_conle {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
}

.recommend_conri {
  padding-left: 22rpx;
  font-size: 28rpx;
  font-weight: 500;
  padding-top: 4rpx;
  color: #999;
}

.recommend_in {
  display: flex;
  margin-bottom: 30rpx;
}

.recommend_li {
  width: 44.8vw;
  padding-top: 20rpx;
}

.recommend_list {
  height: 374rpx;
  width: 44.8vw;
  border: 1px solid #e8e8e8;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  overflow: hidden;
  background: #fff;
}

.recommend_list_img {
  width: 44.8vw;
  height: 202rpx;
  position: relative;
  overflow: hidden;
}

.recommend_list_img >image {
  width: 44.8vw;
  max-width: 44.8vw;
}

.recommend_list_name >text {
  margin-left: -10rpx;
  color: #ff4949;
}

.recommend_list_name {
  padding-top: 6rpx;
  width: 40.6vw;
  margin: 0;
  height: 80rpx;
  font-size: 28rpx;
  color: #333;
  /* font-weight:500; */
  line-height: 1.4;
}

.recommend_list_dat {
  padding: 0 14rpx;
  width: 44.8vw;
}

.recommend_list_pri {
  font-size: 22rpx;
}

.recommend_list_bot {
  font-size: 28rpx;
  color: #e60012;
}

.recommend_list_pr {
  text-decoration: line-through;
}

